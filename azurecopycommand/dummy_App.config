<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>

  <appSettings>
   
    <!-- general -->
    <add key="AzureAccountKey" value="xxxxxx   azure account key xxxxxxxxx"/>
    <add key="AWSAccessKeyID" value="  s3 access key "/>
    <add key="AWSSecretAccessKeyID" value="s3 secret key "/>
    <add key="AWSRegion" value="s3 region "/>

    <!-- source. Orig of copying. -->
    <add key="SrcAzureAccountKey" value="xxxxxx   azure account key xxxxxxxxx"/>
    <add key="SrcAWSAccessKeyID" value="  s3 access key "/>
    <add key="SrcAWSSecretAccessKeyID" value="s3 secret key "/>
    <add key="SrcAWSRegion" value="s3 region "/>

    <!-- target. Destination of copying -->
    <add key="TargetAzureAccountKey" value="xxxxxx   azure account key xxxxxxxxx"/>
    <add key="TargetAWSAccessKeyID" value="  s3 access key "/>
    <add key="TargetAWSSecretAccessKeyID" value="s3 secret key "/>
    <add key="TargetAWSRegion" value="s3 region "/>

    <!-- skydrive -->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="ClientId" value="000000004407DDB4"/>
    <add key="SkyDriveCode" value="please populate"/>
    <add key="SkyDriveRefreshToken" value=""/>


    <!-- blobcopy timeouts -->
    <add key="MaxExecutionTimeInMins" value="60" />
    <add key="MaxServerTimeoutInMins" value="60" />
    <add key="BlobCopyBatchSize" value="5" />
    
  </appSettings>
</configuration>
